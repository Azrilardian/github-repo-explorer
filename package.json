{
  "name": "github-repo-explorer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest --watch",
    "lint:js": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:js:fix": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
    "lint:css": "stylelint **/*.{scss,css}",
    "lint:css:fix": "stylelint --fix **/*.{scss,css}",
    "lint:json": "prettier --check \"./**/*.json\"",
    "lint:json:fix": "prettier --write \"./**/*.json\"",
    "lint": "npm run lint:js && npm run lint:css && npm run lint:json",
    "lint:fix": "npm run lint:js:fix && npm run lint:css:fix && npm run lint:json:fix",
    "commit": "commit",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/material": "^5.11.14",
    "@reduxjs/toolkit": "^1.9.3",
    "@types/node": "18.15.5",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "eslint": "8.36.0",
    "next": "13.2.4",
    "next-redux-wrapper": "^8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-query": "^3.39.3",
    "react-redux": "^8.0.5",
    "redux": "^4.2.1",
    "redux-devtools-extension": "^2.13.9",
